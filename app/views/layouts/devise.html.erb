                </button>
                <%= link_to "iTracker", root_path, class: "navbar-brand" %>
            </div>
          </nav>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
        
            </ul>
            
          <%= yield %>     


  </body>
</div>
<script>

 $(".rotator > div:gt(0)").hide();
 setInterval(function() {
   $('.rotator > div:first')
   .fadeOut(0)
   .next()
   .fadeIn(1000)
   .end()
   .appendTo('.rotator');
}, 3000);
</script>
</html>

